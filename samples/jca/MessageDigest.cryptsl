SPEC MessageDigest
OBJECTS
    java.lang.String digestAlg;
   
	
EVENTS
    g1: getInstance(digestAlg);
    Gets := g1 | g2;

ORDER
    Gets

CONSTRAINTS
    digestAlg in {"SHA-256", "SHA-384", "SHA-512"};
    
REQUIRES
    foo[x]; 
    
ENSURES
	digested[out];
    digested[out];